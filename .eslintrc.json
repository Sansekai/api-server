{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": ["airbnb-base"],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "parser": "babel-eslint",
    "rules": {
        "semi": "off",
        "linebreak-style": "off",
        "indent": ["error", 4],
        "max-len": "off",
        "no-console": "off",
        "no-labels": "off",
        "prefer-destructuring": "off"
    }
}
